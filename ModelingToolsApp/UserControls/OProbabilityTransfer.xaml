<UserControl x:Class="ModelingToolsApp.UserControls.OProbabilityTransfer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"           
             Height="42" Width="102">
    <Grid>
        <Polyline Points="0,20 100,20" Stroke="Black" StrokeDashArray="2,2" ToolTip="概率转移" HorizontalAlignment="Center" VerticalAlignment="Center"></Polyline>
        <Label HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,18">概率转移</Label>
        <Polyline  x:Name="path" Points="0,20 100,20" Stroke="Transparent" ToolTip="概率转移" StrokeThickness="13" 
                  HorizontalAlignment="Center" VerticalAlignment="Center" MouseEnter="Polyline_MouseEnter"></Polyline>
    </Grid>
</UserControl>
